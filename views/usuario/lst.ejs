<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Listagem de  usu√°rios</title>
    <style>
        table, tr, td, th{
            border: solid 1px black;
        }
    </style>
</head>
<body>

    <form method="post">
        <input type="text" name="pesquisar">
        <input type="submit" value="Pesquisar">
    </form>
    <br>
    <table>
        <tr>
            <th>Id</th>
            <th>Nome</th>
            <th>Email</th>
            <th>Senha</th>
            <th>Foto</th>
            <th>Editar</th>
            <th>Deletar</th>
        </tr>
        <% Usuarios.forEach(function(usuario){ %>
            <tr>
                <td><%=usuario._id %></td>
                <td><%=usuario.nome %></td>
                <td><%=usuario.email %></td>
                <td><%=usuario.senha %></td>
                <td><%=usuario.foto %></td>
                <td>edita</td>
                <td>deleta</td>
            </tr>
        <% }) %>
    </table>
</body>
</html>